export class Error {
  message: string;

  #new(message: string) {
    this.message = message;
  }
}

export class IndexOutOfBoundsError extends Error {
  index: i32;
  length: i32;

  #new(index: i32, length: i32) {
    super("Index out of bounds");
    this.index = index;
    this.length = length;
  }
}
