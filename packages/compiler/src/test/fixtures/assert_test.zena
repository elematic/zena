// Test file using the suite/test pattern
import { suite, test, TestContext } from 'zena:test';
import { equal, isTrue, isFalse } from 'zena:assert';

export let tests = suite('assert', () => {
  test('equal passes for same values', (ctx: TestContext): void => {
    equal(1, 1);
    equal(42, 42);
  });
  
  test('isTrue passes for true', (ctx: TestContext): void => {
    isTrue(true);
    isTrue(1 == 1);
  });
  
  test('isFalse passes for false', (ctx: TestContext): void => {
    isFalse(false);
    isFalse(1 == 2);
  });
});
