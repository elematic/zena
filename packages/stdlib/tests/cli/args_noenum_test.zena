// Test getArguments without enum
//
// @requires: wasmtime

import {console} from 'zena:console-wasi';
import {getArguments} from 'zena:cli';

export let main = (): i32 => {
  console.log("Testing getArguments...");
  
  let args = getArguments();
  console.log("Got args");
  
  if (args.length > 0) {
    console.log("First arg: " + args[0]);
  }
  if (args.length > 1) {
    console.log("Second arg: " + args[1]);
  }
  if (args.length > 2) {
    console.log("Third arg: " + args[2]);
  }
  
  return 0;
};
