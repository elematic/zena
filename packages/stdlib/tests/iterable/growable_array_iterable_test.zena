// Growable Array iterable tests using shared test utilities
import { iterableTests } from './iterable_shared.zena';
import { Array } from 'zena:growable-array';
import { FixedArray } from 'zena:fixed-array';
import { Sequence } from 'zena:sequence';

let createGrowableArray = (items: FixedArray<i32>): Sequence<i32> => {
  let arr = new Array<i32>();
  for (let x in items) {
    arr.push(x);
  }
  return arr;
};

export let tests = iterableTests('Array', createGrowableArray);
