// ImmutableArray iterable tests using shared test utilities
import { iterableTests } from './iterable_shared.zena';
import { ImmutableArray } from 'zena:immutable-array';
import { FixedArray } from 'zena:fixed-array';
import { Sequence } from 'zena:sequence';

// Convert FixedArray to ImmutableArray
let createImmutableArray = (items: FixedArray<i32>): Sequence<i32> => {
  return items as ImmutableArray<i32>;
};

export let tests = iterableTests('ImmutableArray', createImmutableArray);
