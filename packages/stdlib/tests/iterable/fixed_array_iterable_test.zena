// FixedArray iterable tests using shared test utilities
import { iterableTests } from './iterable_shared.zena';
import { FixedArray } from 'zena:fixed-array';
import { Sequence } from 'zena:sequence';

// Return items cast to Sequence (FixedArray implements Sequence)
let createFixedArray = (items: FixedArray<i32>): Sequence<i32> => {
  return items as Sequence<i32>;
};

export let tests = iterableTests('FixedArray', createFixedArray);
