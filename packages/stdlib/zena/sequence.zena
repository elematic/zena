import { Iterable } from 'zena:iterator';

export interface Sequence<T> extends Iterable<T> {
  length: i32 { get; }
  operator [](index: i32): T;
  map<U>(f: (item: T, index: i32, seq: this) => U): Sequence<U>;
}

export interface MutableSequence<T> extends Sequence<T> {
  operator []=(index: i32, value: T): void;
}
