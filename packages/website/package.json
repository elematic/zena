{
  "name": "@zena-lang/website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "serve": "eleventy --serve"
  },
  "wireit": {
    "build": {
      "command": "eleventy",
      "files": [
        "src/**/*",
        "eleventy.config.js"
      ],
      "output": [
        "_site/**/*"
      ]
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
