// Test file with a failing test
import { suite, test, TestContext } from 'zena:test';
import { equal, isTrue } from 'zena:assert';

export let tests = suite('Mixed Results', (): void => {
  test('this passes', (ctx: TestContext): void => {
    equal(1, 1);
  });
  
  test('this fails', (ctx: TestContext): void => {
    equal(1, 2);
  });
  
  test('this also passes', (ctx: TestContext): void => {
    isTrue(true);
  });
});
