// @mode: run
// @result: 10

type F1 = (a: i32) => i32;
type F2 = (a: i32, b: i32) => i32;
type U = F1 | F2;

export let main = (): i32 => {
  // Assign a 1-arg function to the union.
  let f: U = (a: i32) => a;

  // Call with 2 args.
  // Should work by adapting F1 (dropping 2nd arg) or calling F2 directly.
  return f(10, 20);
};
