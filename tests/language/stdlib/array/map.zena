// @result: 100
import {Array} from 'zena:array';

export let main = (): i32 => {
  let arr = new Array<i32>(4);
  arr.push(1);
  arr.push(2);
  arr.push(3);
  
  let mapped = arr.map<i32>((x: i32) => x * 2);
  
  if (mapped.length != 3) return 1;
  if (mapped[0] != 2) return 2;
  if (mapped[1] != 4) return 3;
  if (mapped[2] != 6) return 4;
  
  return 100;
};
